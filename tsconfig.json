{
  "compilerOptions": {
    "target": "ES2022", 
    "moduleResolution": "Node",                         
    "module": "commonjs",                    
    "declaration":false,
    "noLib":false,
    "emitDecoratorMetadata":true,
    "experimentalDecorators":true,
    "sourceMap":true,
    "pretty":true,
    "resolveJsonModule":false,
    "allowUnreachableCode":true,
    "allowUnusedLabels":true,
    "noImplicitAny":true,
    "noImplicitReturns":false,
    "noImplicitUseStrict":false,
    "outDir":"build/",
    "baseUrl":"src/",
    "listFiles":false,
    "noEmitHelpers":true
  },
  "compileOnSave":false
}